<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
  <listeners>
  	<listener class-name="Framework.Listeners"/>
  </listeners>
  <test thread-count="5" name="Validate Navigation Bar">
    <classes>
      <class name="Framework.validateNavigation"/>
    </classes>
  </test>
  <test name="Validating Title">
  	<classes>
  		<class name="Framework.validateTitle"/>
  	</classes>
  </test>
  <test name="Validation Login Page">
  	<classes>
    	<class name="Framework.HomePage"/>
    </classes>
  </test>
</suite> 
